{
  "name": "syncternet-demo",
  "version": "1.0.0",
  "description": "Syncternet demo page showcasing the Syncternet NPM package. This project is also a good starting point to develop Syncternet plugins.",
  "author": "www.adelriosantiago.com",
  "license": "MIT",
  "main": "server.js",
  "scripts": {
    "start": "node ./server.js",
    "dev": "nodemon --exec \"npm run serve\"",
    "build:syncternet": "npm run build --prefix ./dev_modules/syncternet",
    "serve": "run-s build:syncternet start"
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "express": "^4.21.2",
    "syncternet": "file:./dev_modules/syncternet",
    "uptill": "^1.0.1"
  },
  "devDependencies": {
    "browserify": "^17.0.1",
    "cheerio": "^1.0.0-rc.5",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0",
    "url": "^0.11.4",
    "uuid": "^11.0.3",
    "vue": "^3.5.13"
  }
}
