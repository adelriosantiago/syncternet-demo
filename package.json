{
  "name": "syncternet-demo",
  "version": "1.0.0",
  "description": "Syncternet demo page showcasing the Syncternet NPM package. This project is also a good starting point to develop Syncternet plugins.",
  "author": "www.adelriosantiago.com",
  "license": "MIT",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node ./server.js",
    "dev": "nodemon --exec \"npm run serve\"",
    "serve": "run-s build:client start",
    "build:client": "npm run build:client --prefix ./dev_modules/syncternet"
  },
  "devDependencies": {
    "body-parser": "^2.2.1",
    "browserify": "^17.0.1",
    "cheerio": "^1.2.0",
    "express": "^5.2.1",
    "nodemon": "^3.1.11",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "syncternet": "file:./dev_modules/syncternet",
    "uptill": "^1.0.1",
    "url": "^0.11.4",
    "uuid": "^13.0.0",
    "vue": "^3.5.26"
  }
}
